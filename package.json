{
  "name": "churrasco-garantido-node",
  "version": "1.0.0",
  "description": "Projeto backend do churrasco garantido desenvolvido em NodeJS",
  "main": "src/index.js",
  "scripts": {
    "build": "npm run clear && gulp build",
    "clear": "rm -rf dist",
    "coveralls": "npm run test:coverage && nyc report --reporter=text-lcov | coveralls",
    "lint": "standard",
    "start": "pm2 start configs/pm2.dev.json",
    "start:prod": "NODE_ENV=production npm run build && npm run start configs/pm2.prod.json",
    "test": "./node_modules/.bin/mocha tests/**/*.tests.js --exit",
    "test:coverage": "nyc npm test",
    "test:dev": "./node_modules/.bin/mocha tests/**/*.tests.js --watch"
  },
  "standard": {
    "ignore": [
      "cnpjValidator.js"
    ],
    "globals": [
      "beforeEach",
      "afterEach",
      "it",
      "context",
      "describe",
      "expect",
      "test"
    ],
    "parser": "babel-eslint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LeonardoHabitzreuter/churrasco-garantido-node.git"
  },
  "author": "Leonardo Habitzreuter <leonardo.h@hbsis.com.br> (https://github.com/LeonardoHabitzreuter)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/LeonardoHabitzreuter/churrasco-garantido-node/issues"
  },
  "homepage": "https://github.com/LeonardoHabitzreuter/churrasco-garantido-node#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.2",
    "cors": "^2.8.4",
    "express": "^4.16.2",
    "express-query-int": "^2.0.0",
    "husky": "^0.15.0-rc.8",
    "jsonwebtoken": "^8.1.1",
    "mongoose": "^5.0.3",
    "morgan": "^1.9.0",
    "node-restful": "^0.2.6",
    "pm2": "^2.10.2"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "chai": "^4.1.2",
    "coveralls": "^3.0.0",
    "gulp": "^4.0.0",
    "gulp-babel": "^7.0.1",
    "gulp-uglify": "^3.0.0",
    "mocha": "^5.0.1",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.14.12",
    "nyc": "^11.5.0",
    "sinon": "^4.3.0",
    "sinon-chai": "^2.14.0",
    "standard": "^11.0.0",
    "supertest": "^3.0.0"
  }
}
